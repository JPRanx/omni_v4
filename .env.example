# OMNI V4 - Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Environment
ENVIRONMENT=dev  # dev, staging, or prod

# Supabase Database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here  # For admin operations

# Processing Configuration
PARALLEL_WORKERS=6  # Number of concurrent tasks (2 days Ã— 3 restaurants)
QUALITY_LEVEL=3     # 1=basic, 2=labor, 3=efficiency

# Paths (override defaults if needed)
# INPUT_PATH=./data/input
# STATE_PATH=./data/state
# LOG_PATH=./data/logs
# CHECKPOINT_PATH=./data/state/checkpoints

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR

# Pattern Learning
PATTERN_LEARNING_ENABLED=true
PATTERN_UPDATE_BATCH_SIZE=100
MIN_PATTERN_CONFIDENCE=0.5

# Observability (optional)
METRICS_ENABLED=true
HEALTH_CHECK_ENABLED=true